   "source": [
    "def binary(x, ans):\n",
    "    if x == 0:\n",
    "        return ans\n",
    "    elif x % 2 == 0:\n",
    "        return binary(x/2, ans)\n",
    "    else:\n",
    "        return binary((x-1)/2, ans+1)"
   ]
